    "scripts": {
        "start": "vite --port 3030",
        "build": "tsc && vite build",
        "serve": "vite preview --port 3030",
        "build:ssr": "npm run build:client && npm run build:server",
        "build:client": "vite build --outDir dist/client",
        "build:server": "vite build --ssr src/entry-server.jsx --outDir dist/server",
        "generate": "vite build --outDir dist/static && npm run build:server && node prerender",
        "serve:ssr": "node server",
        "eject": "react-scripts eject",
        "lint": "eslint src --color",
        "prettier": "prettier src -check",
        "lint-staged": "lint-staged --config lint-staged.js",
        "husky-install": "husky install",
        "format": "prettier --write .",
        "prepare": "husky install",
        "lint:fix": "eslint --fix",
        "test:unit": "vitest --root src/",
        "test": "vitest -- watch",
        "test:ui": "vitest --ui",
        "test:coverage": "vitest --coverage",
        "prettier:format": "prettier --write ./**/*.{js,jsx,ts,tsx,css,md,json} --config ./.prettierrc",
        "eslint:fix": "npx tsc --noEmit && npx eslint --fix . && npx prettier --write ."
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
